{
    "gestures": {
        "thumbs_up": "volume_up()",
        "thumbs_down": "volume_down()",
        "victory_sign": "pause_play()",
        "fist": "mouse_click()",
        "open_hand": "scroll_up()",
        "close_hand": "scroll_down()",
        "swipe_left": "previous_track()",
        "swipe_right": "next_track()",
        "pinch": "increase_brightness()",
        "spread": "decrease_brightness()",
        "point_up": "open_app('notepad')",
        "point_down": "open_app('chrome')",
        "circle": "log_gesture('circle detected')",
        "wave": "print('Wave detected!')",
        "rock_and_roll": "log_gesture('Rock on!')",
        "peace_sign": "log_gesture('Peace!')",
        "two_fingers_swipe_up": "sbc.set_brightness('+20')",
        "two_fingers_swipe_down": "sbc.set_brightness('-20')",
        "ok_sign": "pyautogui.hotkey('ctrl', 's')",
        "call_me": "pyautogui.hotkey('ctrl', 'shift', 'n')",
        "thumbs_sideways": "pyautogui.hotkey('alt', 'tab')",
        "snap_fingers": "pyautogui.hotkey('ctrl', 't')",
        "salute": "pyautogui.hotkey('win', 'd')",
        "point_right": "pyautogui.hotkey('alt', 'right')",
        "point_left": "pyautogui.hotkey('alt', 'left')",
        "tap": "pyautogui.press('enter')",
        "double_tap": "pyautogui.doubleClick()",
        "clap": "pyautogui.hotkey('ctrl', 'p')",
        "zoom_in": "pyautogui.hotkey('ctrl', '+')",
        "zoom_out": "pyautogui.hotkey('ctrl', '-')",
        "rotate_clockwise": "pyautogui.hotkey('ctrl', 'r')",
        "rotate_counterclockwise": "pyautogui.hotkey('ctrl', 'shift', 'r')"
    }
}
